
generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Appointment {
  id        String   @id @default(cuid())
  date      DateTime
  time      String
  client    String
  createdAt DateTime @default(now())
  updatedAt DateTime @updatedAt

  @@map("appointments")
}
